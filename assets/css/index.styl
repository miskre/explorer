@import 'nib'
@import 'fonts'
@import 'remodal'
@import 'gia.ui/gia.ui'
@import '../../node_modules/aos/dist/aos.css'


main-gradient(direction = top)
  background linear-gradient(direction, #0ce4e1, #d855ff)

::selection
  background #0ce4e1
  color white

@import 'whitelist'

@keyframes mis
  0%
    transform scale(1.2)
    opacity .75
  50%
    transform scale(1)
    opacity 1
  100%
    transform scale(1.2)
    opacity .75

@keyframes kre
  0%
    transform scale(1)
    opacity .75
  50%
    transform scale(1.2)
    opacity 1
  100%
    transform scale(1)
    opacity .75

html
body
  overflow-x hidden
  background-color darken(#111, 2)
  &.no-scroll
    overflow hidden
  &.no-js
    #preload
      display none
    [data-aos]
      opacity 1
      transform translate(0) scale(1)
      
span.miskre
  display inline-block
  background-image url(../img/favicon.png)
  background-repeat no-repeat
  background-position left center
  background-size 0.75em
  padding-left 1em
  font-weight bold
  color white
  text-transform lowercase
  margin-left .4em
  margin-right .4em
span.mis
span.kre
  display inline-block
  background-image url(../img/mis.svg)
  background-repeat no-repeat
  background-position left center
  background-size 0.6em
  padding-left 0.8em
  margin-left .4em
  margin-right .4em
  font-weight bold
  color white
  text-transform uppercase
  font-size .9em
span.kre
  background-image url(../img/kre.svg)

.bio-modal
.languages-modal
  text-align left
  background-color rgba(#000, .95)
  max-width 500px
  p
    color white
    font-size 14px
    line-height 1.6em
    margin 0
    &:not(:last-child)
      margin-bottom 15px

.languages-modal
  max-width 250px
  .languages
    a
      display block
      color steel
      background-repeat no-repeat
      height 32px
      background-position center left 10px
      background-size: @height - 16px
      line-height: @height - 16px
      padding: 8px 10px 8px (@height + 10px)
      transition all .4s ease-in-out
      &:hover
        background-color jet
        color white
      &.{active-class}
        color white
      &.es
        background-image url('data:image/svg+xml;utf8;base64,')
      &.de
        background-image url('data:image/svg+xml;utf8;base64,')
      &.zh
        background-image url('data:image/svg+xml;utf8;base64,')
      &.en
        background-image url('data:image/svg+xml;utf8;base64,')
      &.ja
        background-image url('data:image/svg+xml;utf8;base64,')
      &.ko
        background-image url('data:image/svg+xml;utf8;base64,')
      &.ru
        background-image url('data:image/svg+xml;utf8;base64,')

.remodal .x
  display block
  position absolute
  top 0
  right 0
  width 30px
  height @width
  color white
  text-align center
  line-height @height
  main-gradient(45deg)

#menu
  position fixed
  background-color rgba(#111, .975)
  top 0
  right 0
  bottom 0
  left 0
  z-index 1000
  padding 90px 20px 40px 60px
  @media medium
    padding 120px 20px 50px 120px
  @media large
    padding 150px 20px 50px 120px
  ul.navigations
    margin 0
    padding 0
    list-style none
    a
      color jumbo
      font-weight bold
      transition color .5s ease-in-out
      font-size 20px
      // text-transform uppercase
      line-height 30px
      @media medium
        font-size 24px
        text-transform none
        line-height 36px
      &:hover
        color white

#top-bar
  position fixed
  top 0
  left 0
  right 0
  font-size 0
  pointer-events none
  z-index 1000
  padding-top 40px
  padding-bottom 30px
  background linear-gradient(top, rgba(#000, .5), transparent)
  transition top .5s ease-in-out
  &.up
    top -100px
  &.down
    top 0
  @media medium
    padding-top 70px
    padding-bottom 50px
  a
    pointer-events all
  .menu
    display inline-block
    text-align center
    width 60px
    @media medium
      width 120px
  .logo
    display inline-block
  .enter-whitelist
    main-gradient(left)
    display inline-block
    font-weight bold
    -webkit-background-clip text
    -webkit-text-fill-color transparent
    float right
    color white
    font-size 16px
    margin-right 20px
    margin-top 5px

#social-bar
  position fixed
  bottom 0
  left 0
  width 120px
  padding-bottom 70px
  pointer-events none
  z-index 1000
  display none
  @media medium
    display block
  > a
    display block
    text-align center
    pointer-events all
    &:not(:last-child)
      margin-bottom 40px

#explorer
  max-width 1300px
  position relative
  z-index 1
  width 100%
  padding 120px 20px 40px 20px
  color snow
  @media medium
    padding 170px 20px 50px 120px
  @media large
    padding 200px 20px 150px 120px
  &.full-width
    max-width none
  .part-title
    display inline-block
    font-weight bold
    text-transform lowercase
    font-size 18px
    margin 20px 0 15px 0
    main-gradient(left)
    -webkit-background-clip text
    -webkit-text-fill-color transparent
  .txid
  .hash
  .blockhash
    font-family font-family-monospace
    font-weight bold
    font-size 14px
    white-space nowrap
  ul.blocks
  ul.transactions
    margin 0
    padding 15px 20px
    list-style none
    background-color #000
    > li
      &:not(:last-child)
        .stamp
          border-bottom 1px solid #111
  time.ago
    color base
    font-size 11px
    font-weight normal
  a
    color white
  .stamp
    // background-color #000
    padding 10px 0
    position relative
    overflow hidden
    &:hover
      .index
        display block
    .index
      display none
      position absolute
      font-size 60px
      font-weight bold
      color #111
      right -8px
      bottom -35px
  .meta
    display inline-block
    color iron
    font-size 11px
    &:not(:last-child)
      margin-right 10px
    &:before
      text-transform lowercase
      color base
      margin-right 5px
  .type:before
    content 'Type'
  .sys-fee:before
    content 'Sysfee'
  .net-fee:before
    content 'Netfee'
  .size:before
    content 'Size'
  .confirmations:before
    content 'Confirmations'
  .blockhash:before
    content 'Block'
    
